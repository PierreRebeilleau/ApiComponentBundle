{
    "name": "silverbackis/api-component-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Built on API Platform, this bundle provides components, form handling and more for a BW Starter website.",
    "require": {
        "php": "^7.1",
        "api-platform/api-pack": "^1.0.1",
        "cocur/slugify": "^3.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "nelmio/cors-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^4.0",
        "symfony/console": "^4.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/yaml": "^4.0",
        "webonyx/graphql-php": "^0.11.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/dotenv": "^3.4 || ^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/maker-bundle": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\Tests\\": "tests/"
        }
    }
}
