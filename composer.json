{
    "name": "silverbackis/api-component-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "api-platform/api-pack": "^1.2",
        "api-platform/core": "^2.5.4",
        "cocur/slugify": "^3.0",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "gesdinet/jwt-refresh-token-bundle": "^0.9.1",
        "lexik/jwt-authentication-bundle": "^2.6",
        "liip/imagine-bundle": "^2.3",
        "ocramius/proxy-manager": "^2.6",
        "ramsey/uuid": "^3.9",
        "symfony/console": "4.4.*",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/mailer": "^5.0|^4.4",
        "twig/cssinliner-extra": "^3.0",
        "twig/extra-bundle": "^3.0",
        "twig/inky-extra": "^3.0"
    },
    "require-dev": {
        "justinrainbow/json-schema": "^5.2",
        "roave/security-advisories": "dev-master",
        "symfony/debug": "^4.4",
        "symfony/dotenv": "^4.4",
        "symfony/http-client": "4.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1",
        "symfony/phpunit-bridge": "^5.0.3 || dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*",
            "root-dir": "tests/app"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
