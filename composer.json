{
    "name": "silverbackis/api-component-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Built on API Platform, this bundle provides components, form handling and more for a BW Starter website.",
    "require": {
        "php": "^7.1",
        "api-platform/api-pack": "^1.0.1",
        "api-platform/core": "^2.2.0@beta",
        "cocur/slugify": "^3.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "enqueue/amqp-ext": "^0.8.12",
        "enqueue/enqueue-bundle": "^0.8.17",
        "guzzlehttp/guzzle": "^6.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "liip/imagine-bundle": "2.0.x-dev@dev",
        "nelmio/cors-bundle": "^1.5",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^4.0",
        "symfony/console": "^4.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/orm-pack": "^1.0",
        "symfony/proxy-manager-bridge": "^4.0",
        "symfony/serializer": "^4.0@dev",
        "symfony/yaml": "^4.0",
        "roave/security-advisories": "dev-master"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "dev-master@dev",
        "behat/mink-extension": "^2.2",
        "behat/symfony2-extension": "^2.1.1",
        "behatch/contexts": "dev-master",
        "phpunit/phpunit": "^6.0",
        "symfony/dotenv": "^3.4 || ^4.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\Tests\\": "tests/"
        }
    }
}
